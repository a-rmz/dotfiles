{
  "workbench.colorTheme": "Outrun Electric",
  "window.zoomLevel": 1,
  "editor.fontSize": 14,
  "files.autoSave": "onWindowChange",
  "editor.wordWrap": "on",
  "python.venvPath": "~/.pyenv",
  "files.exclude": {
    "**/__pycache__": true,
    "**/.pyc": true
  },
  "workbench.iconTheme": "material-icon-theme",
  "python.languageServer": "Pylance",
  "editor.formatOnSave": true,
  "python.formatting.provider": "black",
  "editor.fontFamily": "'JetBrains Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.inlineHints.fontFamily": "'JetBrains Mono', Menlo, Monaco, 'Courier New', monospace",
  "[json]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "[vue]": {
    "editor.defaultFormatter": "octref.vetur"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": ["javascript", "vue"],
  "customizeUI.activityBar": "wide",
  "customizeUI.font.regular": "JetBrains Mono",
  "customizeUI.font.monospace": "JetBrains Mono",
  "customizeUI.titleBar": "inline",
  "window.titleBarStyle": "native"
}
